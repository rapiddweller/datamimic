<setup multiprocessing="0">
    <generate name="bank" count="10" target="ConsoleExporter">
        <variable name="bank" entity="Bank"/>
        <key name="name" script="bank.name"/>
        <key name="name1" script="bank.name"/>
        <key name="bank_code" script="bank.bank_code"/>
        <key name="bank_code1" script="bank.bank_code"/>
        <key name="bic" script="bank.bic"/>
        <key name="bic1" script="bank.bic"/>
        <key name="bin" script="bank.bin"/>
        <key name="bin1" script="bank.bin"/>
        <variable name="bank1" entity="Bank"/>
        <key name="name2" script="bank1.name"/>
        <key name="name3" script="bank1.name"/>
        <key name="bank_code2" script="bank1.bank_code"/>
        <key name="bank_code3" script="bank1.bank_code"/>
        <key name="bic2" script="bank1.bic"/>
        <key name="bic3" script="bank1.bic"/>
        <key name="bin2" script="bank1.bin"/>
        <key name="bin3" script="bank1.bin"/>
    </generate>

    <!-- WHY: Cover dataset overrides for banks so integration assertions can ensure locale-specific codes remain stable. -->
    <echo>Generating German banks</echo>
    <generate name="seeded_de_banks" count="3" target="LogExporter">
        <variable name="bank_de" entity="Bank" dataset="DE"/>
        <key name="name" script="bank_de.name"/>
        <key name="bic" script="bank_de.bic"/>
        <key name="bank_code" script="bank_de.bank_code"/>
    </generate>
</setup>
