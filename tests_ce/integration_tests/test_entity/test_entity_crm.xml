        <key name="customer_id2" script="crm1.customer_id" />
        <key name="customer_id3" script="crm1.customer_id" />
        <key name="given_name2" script="crm1.given_name" />
        <key name="given_name3" script="crm1.given_name" />
        <key name="family_name2" script="crm1.family_name" />
        <key name="family_name3" script="crm1.family_name" />
        <key name="email2" script="crm1.email" />
        <key name="email3" script="crm1.email" />
        <key name="segment2" script="crm1.segment" />
        <key name="segment3" script="crm1.segment" />
    </generate>
</setup> 