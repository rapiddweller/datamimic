<setup multiprocessing="0">
    <generate name="crm" count="10" target="ConsoleExporter">
        <variable name="crm" entity="CRM" />
        <key name="customer_id" script="crm.customer_id" />
        <key name="customer_id1" script="crm.customer_id" />
        <key name="first_name" script="crm.first_name" />
        <key name="first_name1" script="crm.first_name" />
        <key name="last_name" script="crm.last_name" />
        <key name="last_name1" script="crm.last_name" />
        <key name="email" script="crm.email" />
        <key name="email1" script="crm.email" />
        <key name="phone" script="crm.phone" />
        <key name="phone1" script="crm.phone" />
        <key name="customer_since" script="crm.customer_since" />
        <key name="customer_since1" script="crm.customer_since" />
        <key name="segment" script="crm.segment" />
        <key name="segment1" script="crm.segment" />
        <key name="lifetime_value" script="crm.lifetime_value" />
