<setup multiprocessing="0">
    <generate name="bank_account" count="10" target="ConsoleExporter">
        <variable name="bank_account" entity="BankAccount"/>
        <key name="account_number" script="bank_account.account_number"/>
        <key name="account_number1" script="bank_account.account_number"/>
        <key name="bank_name" script="bank_account.bank_name"/>
        <key name="bank_name1" script="bank_account.bank_name"/>
        <key name="bank_code" script="bank_account.bank_code"/>
        <key name="bank_code1" script="bank_account.bank_code"/>
        <key name="bic" script="bank_account.bic"/>
        <key name="bic1" script="bank_account.bic"/>
        <key name="bin" script="bank_account.bin"/>
        <key name="bin1" script="bank_account.bin"/>
        <key name="iban" script="bank_account.iban"/>
        <key name="iban1" script="bank_account.iban"/>
        <variable name="bank_account1" entity="BankAccount"/>
        <key name="account_number2" script="bank_account1.account_number"/>
        <key name="account_number3" script="bank_account1.account_number"/>
        <key name="bank_name2" script="bank_account1.bank_name"/>
        <key name="bank_name3" script="bank_account1.bank_name"/>
        <key name="bank_code2" script="bank_account1.bank_code"/>
        <key name="bank_code3" script="bank_account1.bank_code"/>
        <key name="bic2" script="bank_account1.bic"/>
        <key name="bic3" script="bank_account1.bic"/>
        <key name="bin2" script="bank_account1.bin"/>
        <key name="bin3" script="bank_account1.bin"/>
        <key name="iban2" script="bank_account1.iban"/>
        <key name="iban3" script="bank_account1.iban"/>
    </generate>

    <!-- WHY: Exercise dataset-specific bank accounts for integration checks on currency and account type distributions. -->
    <echo>Generating French bank accounts</echo>
    <generate name="fr_bank_accounts" count="5" target="LogExporter">
        <variable name="fr_account" entity="BankAccount" dataset="FR"/>
        <key name="account_number" script="fr_account.account_number"/>
        <key name="account_type" script="fr_account.account_type"/>
        <key name="currency" script="fr_account.currency"/>
        <key name="bank_name" script="fr_account.bank_name"/>
        <key name="iban" script="fr_account.iban"/>
    </generate>
</setup>
