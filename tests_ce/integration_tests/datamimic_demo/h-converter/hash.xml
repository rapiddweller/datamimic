<setup>

    <generate name="HashConverter" count="1" target="ConsoleExporter">
        <key name="converter" constant="MD5Hash"/>
        <key name="value" constant="example text to hash"/>
        <key name="value_hashed" script="value" converter="Hash('md5','hex')"/>
    </generate>

