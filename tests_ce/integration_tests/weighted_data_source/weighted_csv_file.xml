<setup defaultSeparator=",">
    <generate name="people_from_source" count="1000" target="ConsoleExporter">
        <variable name="people" source="data/people.wgt.ent.csv"/>
        <key name="people_id" script="people.id"/>
        <key name="people_name" script="people.name"/>
        <key name="people_age" script="people.age"/>
        <key name="active" type="bool" source="data/active.wgt.csv"/>
    </generate>
</setup>