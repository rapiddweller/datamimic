<setup multiprocessing="1" numProcess="2">

    <ml-train name="customer_csv_gen"
              source="data/customer.ent.csv"
              mode="persist"
              maxTrainingTime="5"/>

    <!-- Generate synthetic CUSTOMER records using the ML generator -->
    <generate name="NEW_CUSTOMER" count="10000" pageSize="1000" source="customer_csv_gen" target="CSV">
        <key name="id" generator="IncrementGenerator"/>
    </generate>
</setup>
