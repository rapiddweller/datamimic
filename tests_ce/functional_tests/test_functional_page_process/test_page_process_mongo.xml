<setup multiprocessing="True">
    <mongodb id="mongodb"/>
    <!-- clear mongo_func_test database -->
    <generate name="delete" source="mongodb" selector="find: 'motorcycles', filter: {}" target="mongodb.delete">
    </generate>
    <generate name="motorcycles" count="100" target="mongodb">
        <key name="id" type="int" generator="IncrementGenerator"/>
        <key name="year" type="int" values="1970, 2023"/>
        <nestedKey name="attributes" type="dict" condition="year &lt; 2000">
            <key name="color" type="string"/>
            <key name="serial" type="int" condition="id % 2 == 1"/>
        </nestedKey>
        <list name="list">
            <item condition="year &lt; 2000 and id % 2 == 1">
                <key name="ele_id" script="attributes.serial"/>
            </item>
            <item>
                <key name="ele_year" script="year"/>
            </item>
        </list>
    </generate>
    <generate name="motorbike" type="motorcycles" source="mongodb"/>
</setup>