<setup multiprocessing="0" numProcess="2">

    <database id="sourceDB" system="postgresql"/>

    <include uri="conf/base.properties"/>

    <!-- Run database setup scripts to create schema and tables -->
    <execute uri="script/setup_postgresql.scr.sql" target="sourceDB"/>

    <generate name="pre-gen" type="sequence_table_generator" count="10" target="sourceDB, ConsoleExporter">
        <key name="name" type="string"/>
    </generate>
    <generate name="seq-gen" type="sequence_table_generator" count="10" target="sourceDB, ConsoleExporter"
              multiprocessing="1" numProcess="2">
        <variable name="stop_id" generator="IncrementGenerator(1,5)"/>
        <key name="id" database="sourceDB" generator="SequenceTableGenerator"/>
        <key name="name" type="string"/>
    </generate>
    <generate name="post-gen" type="sequence_table_generator" count="10" target="sourceDB, ConsoleExporter">
        <key name="name" type="string"/>
    </generate>
    <generate name="check" type="sequence_table_generator" source="sourceDB" target="ConsoleExporter"
    />
</setup>
